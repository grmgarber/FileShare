%h3 All Uploads

- unless @uploads.empty?
  %table
    %thead
    %tbody
      %tr
        %th File Name
        %th Description
        %th Meta Updated At
        %th= link_to 'New Upload', new_upload_path
      - @uploads.each do |u|
        %tr
          %td u.upl_file_file_name
          %td= u.description
          %td= u.updated_at
          %td
            = link_to 'Show', upload_path(u)
            = link_to 'Edit', edit_upload_path(u)
            = link_to 'Delete', upload_path(u), method: :delete
